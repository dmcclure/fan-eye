<div class="container">
    <h2>Current Top 5</h2>

    <div class="row">
        <div class="col-md-1"></div>
        {% for item in top5 %}
            <div class="col-md-2 top5">
                <h4>{{ item.name }}</h4>
                <img class="cover" src="{{ asset('images/') }}{{ item.image }}">
                <p>{{ item.percentageWon }}%</p>
            </div>
        {% endfor %}
        <div class="col-md-1"></div>
    </div>
</div>
